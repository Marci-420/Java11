<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="147.0" prefWidth="207.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sudoku_.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Új feladvány mérete:" />
    <HBox alignment="BASELINE_CENTER" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Button fx:id="minus" alignment="TOP_CENTER" onAction="#minusButtonClick" prefHeight="25.0" prefWidth="3.0" text="-" textAlignment="CENTER" />
            <Label fx:id="size" text="4" textAlignment="CENTER">
                <opaqueInsets>
                    <Insets />
                </opaqueInsets>
                <HBox.margin>
                    <Insets left="10.0" right="10.0" />
                </HBox.margin></Label>
            <Button fx:id="plus" mnemonicParsing="false" onAction="#plusButtonClick" text="+" textAlignment="RIGHT" />
        </children>
    </HBox>
    <Label text="Kezdőállapot:" />
    <TextField fx:id="nums" onAction="#sizetextfield" />
    <HBox prefHeight="100.0" prefWidth="200.0">
        <children>
            <Label text="Hossz:">
                <HBox.margin>
                    <Insets />
                </HBox.margin>
                <padding>
                    <Insets right="10.0" />
                </padding>
            </Label>
            <Label fx:id="size2" text="0" />
        </children></HBox>
    <Button mnemonicParsing="false" text="Check" />
</VBox>
